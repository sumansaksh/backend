<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
      <!-- <link rel="stylesheet" href="css/product.css"/> -->
      <link rel="stylesheet" href="css/product.css">
</head>
<body class="background-color">
    <div>
   <p>product page</p> 
  <% products.forEach(product =>{%>
    <p onclick="clickedProductName('<%= JSON.stringify(product) %>')">
    <%= product.name %></p>
   
    <p><%= product.price %></p>
    <p><%= product.type %></p>
    <p><%= product.category %></p>
    <img src="<%= product.image %>" alt="suman">
    
  

    <p>
        <a href="products/<%= product._id %>">click here</a>
    </p>
   
    <% });  %>
</div>

    <script src="js/product.js"></script>
</body>
</html>